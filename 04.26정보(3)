a = input("first number : ")
b = input("연산자")
c = input("second number : ")
a = float(a)
c = float(c)
if b == "+":
  print(a,"+",c,"=",a+c)
elif b == "-":
  print(a,"-",c,"=",a-c)
elif b == "*":
  print(a,"*",c,"=",a*c)
elif b == "**":
  print(a,"**",c,"=",a**c)
elif b == "/":
  print(a,"/",c,"=",a/c)
elif b == "//":
  print(a,"//",c,"=",a//c)
elif b == "%":
  print(a,"%",c,"=",a%c)
else :
  print("연산자 입력 오류")
